{
  "name": "Whiteboard",
  "version": "0.0.1",
  "author": "Thomas Winckell",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "classnames": "^2.1.3",
    "detectcss": "^1.0.2",
    "es6-promise": "~2.0.1",
    "firebase": "2.2.7",
    "font-awesome": "^4.4.0",
    "google-maps-api": "^2.0.0",
    "jquery": "^2.1.4",
    "lodash": "3.10.0",
    "moment": "^2.10.6",
    "react": "^0.14.3",
    "react-color": "^1.3.0",
    "react-dom": "^0.14.3",
    "react-tooltip": "^0.9.8",
    "react-webcam": "0.0.8",
    "recordrtc": "^5.2.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.13",
    "babel-eslint": "1.0.12",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.0",
    "del": "~0.1.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "node-libs-browser": "^0.5.3",
    "node-sass": "^3.4.2",
    "opn": "^3.0.3",
    "postcss-loader": "^0.8.0",
    "pretty-hrtime": "*",
    "proxy-middleware": "^0.13.0",
    "resolve-url-loader": "^1.4.2",
    "rimraf": "^2.4.4",
    "run-sequence": "~0.3.5",
    "sass-loader": "^3.1.2",
    "string-replace-webpack-plugin": "0.0.3",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "url": "^0.10.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-notifier": "^1.2.1"
  },
  "scripts": {
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "prewebpack": "npm run clean",
    "webpack": "webpack --colors --display-error-details --progress --config ./webpack/config",
    "start": "NODE_ENV=dev node ./webpack/dev-server  --colors --display-error-details --progress",
    "build": "npm run webpack",
    "prepublish": "npm run build",
    "publish": "aws s3 cp $npm_package_config_buildDir $npm_package_config_aws_s3_bucket --recursive --exclude \"*.DS_Store\""
  },
  "config": {
    "buildDir": "./build",
    "devHost": "localhost",
    "devPort": 9786,
    "aws": {
      "s3": {
        "bucket": "PATH_TO_YOUR_AWS_BUCKET"
      }
    },
    "firebase": {
      "url": "YOUR_FIREBASE_URL"
    },
    "google": {
      "maps": {
        "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
      }
    }
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
