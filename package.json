  },
  "dependencies": {
    "airflux": "^0.5.1",
    "babel-polyfill": "^6.5.0",
    "classnames": "^2.2.3",
    "detectcss": "^1.0.2",
    "firebase": "2.4.1",
    "google-maps-api": "^2.0.1",
    "jquery": "^2.2.0",
    "lodash": "^4.5.0",
    "lodash.range": "^3.1.4",
    "moment": "^2.11.2",
    "rc-slider": "^3.7.2",
    "react": "15.1",
    "react-color": "^2.2.0",
    "react-dom": "15.1",
    "react-motion": "^0.4.3",
    "react-notification-system": "^0.2.7",
    "react-tooltip": "3.0.0",
    "react-webcam": "^0.0.9",
    "recordrtc": "^5.2.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "node-sass": "^3.4.2",
    "opn": "^4.0.0",
    "postcss-loader": "^0.8.1",
    "pretty-hrtime": "^1.0.2",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-notifier": "^1.2.1"
  },
  "scripts": {
    "prewebpack": "rimraf $npm_package_config_demoBuildDir",
    "webpack": "webpack --colors --display-error-details --progress --config ./webpack/config",
    "start": "NODE_ENV=dev node ./webpack/dev-server  --colors --display-error-details --progress",
    "start_windows": "set NODE_ENV=dev&&node ./webpack/dev-server  --colors --display-error-details --progress",
    "start_windows_prod": "set NODE_ENV=production&&node ./webpack/dev-server  --colors --display-error-details --progress",
    "prebuilddemo": "rimraf $npm_package_config_demoBuildDir && mkdir $npm_package_config_demoBuildDir",
    "builddemo": "npm run webpack",
    "prebuild": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "build": "cp -R src/* $npm_package_config_buildDir && cp package.json $npm_package_config_buildDir && cp Readme.md $npm_package_config_buildDir && cp .npmrc $npm_package_config_buildDir",
    "prebuild_windows": "rimraf build && mkdir build",
    "build_windows": "cp -R src/* build && cp package.json build && cp Readme.md build && cp .npmrc build"
  },
  "config": {
    "buildDir": "./build",
    "demoBuildDir": "./demobuild",
    "devHost": "localhost",
    "devPort": 9786
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  }
}
